{
	"Convergence": {
		"domain": "",
		"php": ""
	},

	"type": {
		"core": "TheBASE",
		"hint": "default TheBASE Shopify"
	},

	"preprocessor": {
		"minify_html"	: 1,
		"comment"		: 0,
		"pug"			: 1,
		"php"			: 0
	},

	"css": {
		"minify"		: 1,
		"purge"			: 1,
		"ignore": {
			"js"		: 0,
			"html"		: 0,
			"pug"		: 0,
			"php"		: 0
		}
	},

	"js": {
		"minify"		: 1,
		"encrypt"		: 0,
		"encrypt_level"	: 0
	},

	"img": {
		"minify"		: 0
	},

	"copy": {
		"fontawesome"	: 0,
		"lightgallery"	: 1
	}
}
